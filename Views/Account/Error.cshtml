@model string[]
@{
    ViewBag.Title = "Error";
}

<div class="container mt-4">
    <div class="panel panel-danger">
        <div class="panel-heading">
            <h3 class="panel-title">Error</h3>
        </div>
        <div class="panel-body">
            @foreach (var err in Model)
            {
                <p>@err</p>
            }
        </div>
    </div>
</div>
